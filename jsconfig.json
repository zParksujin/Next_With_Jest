{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "paths": {
            "@/components/*": [
                "components/*"
            ],
            "@/pages/*": [
                "pages/*"
            ],
            "@/hooks/*": [
                "hooks/*"
            ],
            //   "@/styles/*": ["styles/*"]
        }
    },
    "include": [
        "**/*.js",
        "**/*.jsx"
    ],
    "exclude": [
        "node_modules"
    ]
}
